mixin view()
    section.view
        .progress__image-bg 
            img(src="img/sprite/bg-left.svg").progress__img-content
        .mainly__bg
            img(src="img/sprite/bg-right.svg").mainly__img 
        .view__content 
            h1.view__title.view__title--mobile Пост подкачки шин ATIS 210
            .view__inner
                .view__sliders
                    <div class="swiper mySwiper2">
                        <div class="swiper-wrapper">
                            <div class="swiper-slide">
                                a(href="img/content/slide1.png" data-fancybox="galary")
                                    <img src="img/content/slide1.png" alt="Image 1" class="view__img">
                            </div>
                            <div class="swiper-slide">
                                a(href="img/content/slide1.png" data-fancybox="galary")
                                    <img src="img/content/slide1.png" alt="Image 2" class="view__img">
                            </div>
                            <div class="swiper-slide">
                                a(href="img/content/slide1.png" data-fancybox="galary")
